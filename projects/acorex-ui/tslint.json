{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "ax",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ax",
      "kebab-case"
    ],
    "no-inferrable-types": false,
    "member-ordering": false,
    "variable-name": false,
    "use-lifecycle-interface": false,
    "no-output-native": false,
    "eofline":false,
    "one-line" : false
  }
}